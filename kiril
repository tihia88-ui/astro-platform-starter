  <script>
    // динамична година
    document.getElementById('year').textContent = new Date().getFullYear();

    // динамична дата "Актуално към"
    (function(){
      const el = document.querySelector('#asof > span');
      if(el){ el.textContent = new Date().toLocaleDateString('bg-BG'); }
    })();

    // проста форма: генерира mailto с попълнени данни
    function sendQuote(e){
      e.preventDefault();
      const data = Object.fromEntries(new FormData(e.target).entries());
      const subject = encodeURIComponent('Запитване — ' + (data.service || 'услуга'));
      const body = encodeURIComponent(`Име: ${data.name}\nТелефон: ${data.phone}\nИмейл: ${data.email||''}\nНаселено място: ${data.city||''}\nУслуга: ${data.service||''}\nПродължителност: ${data.duration||''}\nБележки: ${data.notes||''}`);
      window.location.href = `mailto:info@stroiplus.bg<!-- TODO: замени с реален имейл, ако е различен -->?subject=${subject}&body=${body}`;
      return false;
    }
  </script>

